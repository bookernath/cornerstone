{
  "name": "catalyst-checkout-theme",
  "version": "1.0.0",
  "template_engine": "handlebars_v4",
  "meta": {
    "price": 0,
    "documentation_url": "https://github.com/bigcommerce/catalyst",
    "author_name": "BigCommerce",
    "author_email": "support@bigcommerce.com",
    "author_support_url": "https://support.bigcommerce.com/",
    "composed_image": "composed.png",
    "features": [
      "catalyst_fallback_theme",
      "customized_checkout",
      "account_payment_methods_v2",
      "gift_certificates"
    ]
  },
  "css_compiler": "scss",
  "autoprefixer_cascade": true,
  "autoprefixer_browsers": [
    "> 1%",
    "last 2 versions",
    "Firefox ESR"
  ],
  "settings": {
    "catalyst_storefront_url": {
      "type": "text",
      "label": "Catalyst Storefront URL",
      "force_reload": true,
      "default": ""
    },
    "redirect_delay": {
      "type": "range",
      "label": "Redirect Delay (milliseconds)",
      "min": 0,
      "max": 5000,
      "step": 100,
      "default": 1000
    },
    "enable_debug_mode": {
      "type": "checkbox",
      "label": "Enable Debug Mode (shows redirect notices)",
      "default": false
    },
    "catalyst_colors": {
      "type": "heading",
      "label": "Catalyst Color Tokens"
    },
    "color-primary": {
      "type": "color",
      "label": "Primary Color",
      "default": "hsl(96, 100%, 68%)"
    },
    "color-accent": {
      "type": "color",
      "label": "Accent Color", 
      "default": "hsl(96, 100%, 88%)"
    },
    "color-background": {
      "type": "color",
      "label": "Background Color",
      "default": "hsl(0, 0%, 100%)"
    },
    "color-foreground": {
      "type": "color",
      "label": "Foreground Color",
      "default": "hsl(0, 0%, 7%)"
    },
    "color-success": {
      "type": "color",
      "label": "Success Color",
      "default": "hsl(116, 78%, 65%)"
    },
    "color-error": {
      "type": "color",
      "label": "Error Color",
      "default": "hsl(0, 100%, 60%)"
    },
    "color-warning": {
      "type": "color",
      "label": "Warning Color",
      "default": "hsl(40, 100%, 60%)"
    },
    "color-info": {
      "type": "color",
      "label": "Info Color",
      "default": "hsl(220, 70%, 45%)"
    },
    "color-black": "#000000",
    "color-white": "#ffffff",
    "color-textBase": "#333333",
    "color-textBase--hover": "#757575",
    "color-textBase--active": "#757575",
    "color-textSecondary": "#757575",
    "color-textSecondary--hover": "#333333",
